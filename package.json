{
  "name": "SmartScannerML",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "generate": "npx react-native generate-bootsplash ./src/assets/logo/ericsson_logo.png --background-color=2EE59D   --logo-width=100 --assets-path=./src/assets --flavor=main"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/checkbox": "^0.5.20",
    "@types/lodash": "^4.17.20",
    "@types/react-native-vector-icons": "^6.4.18",
    "buffer": "^6.0.3",
    "eslint-plugin-react-native": "^5.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-app-auth": "^8.1.0",
    "react-native-input-spinner": "^1.8.1",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^10.3.0",
    "scandit-react-native-datacapture-barcode": "^6.8.0",
    "scandit-react-native-datacapture-core": "^6.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/cli": "^6.4.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^30.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^26.6.3",
    "metro": "^0.66.2",
    "metro-config": "^0.66.2",
    "metro-core": "^0.66.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "metro-react-native-babel-transformer": "^0.66.2",
    "metro-transform-worker": "^0.66.2",
    "prettier": "^3.6.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "setupFiles": [
      "./jestSetupFile.js"
    ],
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-input-spinner)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "react-native/react-native": true,
      "jest": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:react-native"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  }
}
